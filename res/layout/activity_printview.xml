<?xml version="1.0" encoding="utf-8"?>
 
<RelativeLayout 
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/printviewlayout"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
 >
    <EditText
       android:id="@+id/etprintview"
       android:layout_width="fill_parent"
       android:layout_height="300dp"
       android:layout_margin="10dp"
       android:background="@drawable/borderfocus"
       android:scrollbars="vertical"
       android:scrollbarAlwaysDrawVerticalTrack="true"
       android:editable="false"
       android:textColor="#000000"
       android:inputType="textMultiLine"
       />
    <Button 
        android:id="@+id/btnprint"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/etprintview"
        android:layout_marginTop="20dp"
        android:layout_centerHorizontal="true"
        android:text="@string/sprint"
        android:textSize="25sp"
        android:textStyle="bold"
        android:ems="10"
        android:background="@drawable/btnclick_selector"
        android:onClick="OnClickprint"
        />
</RelativeLayout>
